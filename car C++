#include <iostream>
#include <string>
#include <vector>
// Define basic car components as classes
class Engine {
public:
std::string type;
int horsepower;
 Engine(std::string t, int hp) : type(t), horsepower(hp) {}
void start() {
std::cout << "Engine (" << type << ") started. Vroom vroom!\n";
  }
};
class Wheel {
public:
std::string tireType;
int diameter;
Wheel(std::string tt, int d) : tireType(tt), diameter(d) {}
};
class Body {
public:
std::string style;
