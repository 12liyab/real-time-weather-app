#include <iostream>
#include <string>
#include <vector>
// Define basic car components as classes
class Engine {
public:
std::string type;
int horsepower;
 Engine(std::string t, int hp) : type(t), horsepower(hp) {}
void start() {
std::cout << "Engine (" << type << ") started. Vroom vroom!\n";
  }
};
